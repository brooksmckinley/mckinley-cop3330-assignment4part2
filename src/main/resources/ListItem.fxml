<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" prefHeight="6.0" prefWidth="631.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <CheckBox fx:id="checkbox" maxWidth="1.7976931348623157E308" mnemonicParsing="false">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
      </CheckBox>
      <Label fx:id="taskDescription" maxWidth="1.7976931348623157E308" text="Task Description" HBox.hgrow="ALWAYS" />
      <Label fx:id="dueDate" minWidth="128.0" text="Due Date: 2021-10-07">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
      </Label>
      <Button alignment="TOP_LEFT" mnemonicParsing="false" text="Edit" />
      <Button mnemonicParsing="false" text="-" />
   </children>
   <padding>
      <Insets left="5.0" right="5.0" />
   </padding>
</HBox>
